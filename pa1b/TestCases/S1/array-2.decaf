class Main {
    static int f(int i) {
        return i % 4 + 1;
    }

    static void main() {
        var arr1 = [1,2,3] ++ [] ++ [4,5,6];
        var arr2 = arr1[2:5] ++ 2 * 5 %% 5 * 2;
        var arr3 = arr2[arr1[0]:arr1[7] default 10];
        var arr4 = [f(x) for x in arr1];
        foreach (var x in arr4) {
            Print(x);
        }

        var arr5 = [x for x in arr2 if x % 2 == 0];
        foreach (int x in arr5 while x < 10) {
            Print(x);
        }
    }
}